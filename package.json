{
  "name": "vue3-text-editor",
  "version": "1.0.1",
  "main": "./index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build -w",
    "build:lib": "vite build -c ./build/lib.config.ts",
    "build:worker": "vite build -c ./build/worker-lib.config.ts",
    "run:plugin": "vue-tsc  ./build/plugin/vite-plugin-dts-file.ts",
    "serve": "vite preview"
  },
  "dependencies": {
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/mouse-wheel": "^2.4.2",
    "element-plus": "^2.1.8",
    "quill": "1.3.6",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "vue3-text-editor": "^1.0.2",
    "@types/node": "^17.0.23",
    "@types/quill": "^2.0.9",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-critical": "^1.0.8",
    "typescript": "^4.3.2",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.4.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.0.5",
    "vite-plugin-static-copy": "^0.4.3",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^0.2.2"
  }
}
